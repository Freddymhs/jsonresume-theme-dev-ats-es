{{#if resume.volunteer.length}}
<section id="volunteer">
    <h2>Experiencia de Voluntariado</h2>
    {{#each resume.volunteer}}
        {{#> item title=organization subtitle=position startDate=startDate endDate=endDate}}
        {{#if summary}}
        <div class="summary">
            <p>{{summary}}</p>
        </div>
        {{/if}}

        {{#if highlights.length}}
        <ul class="highlights">
            {{#each highlights}}
            <li>{{.}}</li>
            {{/each}}
        </ul>
        {{/if}}
        {{/item}}
    {{/each}}
</section>
{{/if}}
